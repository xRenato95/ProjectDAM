<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#b6abad"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="dam.projects.projectdam.gui.events.PropertiesEventAdminActivity"
    tools:showIn="@layout/activity_properties_event_admin">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/event_list_name"
        android:id="@+id/title_event_name"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:gravity="center_vertical"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/event_list_desc"
        android:id="@+id/title_event_desc"
        android:layout_alignParentStart="true"
        android:gravity="center_vertical"
        android:layout_below="@+id/title_event_name"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/event_list_initial_date"
        android:id="@+id/title_event_initial_date"
        android:layout_below="@+id/title_event_desc"
        android:layout_alignParentStart="true"
        android:gravity="center_vertical"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/event_final_date"
        android:id="@+id/title_event_final_date"
        android:layout_below="@+id/title_event_initial_date"
        android:layout_alignParentStart="true"
        android:gravity="center_vertical"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/title_event_desc"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/title_event_name"
        android:gravity="center_vertical" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView"
        android:layout_alignBottom="@+id/title_event_desc"
        android:layout_alignParentEnd="true"
        android:gravity="center_vertical"
        android:layout_toEndOf="@+id/title_event_desc" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView13"
        android:gravity="center_vertical"
        android:layout_above="@+id/textView15"
        android:layout_alignTop="@+id/title_event_initial_date"
        android:layout_toEndOf="@+id/title_event_initial_date" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView11"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignParentEnd="true"
        android:gravity="center_vertical"
        android:layout_toEndOf="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView15"
        android:layout_below="@+id/title_event_initial_date"
        android:layout_alignBottom="@+id/title_event_final_date"
        android:gravity="center_vertical"
        android:layout_alignEnd="@+id/textView13"
        android:layout_toEndOf="@+id/title_event_initial_date" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView16"
        android:layout_below="@+id/textView13"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:gravity="center_vertical"
        android:layout_toEndOf="@+id/textView13" />
</RelativeLayout>
